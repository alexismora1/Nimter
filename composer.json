{
	"config": {
        "vendor-dir": "src/vendor"
    },
	"name": "alxsmora/nimter",
	"type": "library",
	"description": "microFramework PHP",
	"keywords": ["Framework", "MVC", "PHP"],
	"license": "MIT",
	"authors": [
		{
			"name": "Alexis Mora <alexis.mora1v@gmail.com>"
		}
	],
	"require": {
		"php": ">=7.1.3",
		"symfony/yaml": "^4.2",
		"twig/twig": "2.12.1",
		"vlucas/phpdotenv": "^3.6"
	},
	"require-dev": {
		"symfony/var-dumper": "^4.2",
		"symfony/debug": "^4.2"
	},
    "autoload": {
    	"psr-4": {
    		"Nimter\\": "src/",
    		"Tests\\": "tests/"
    	}
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}